# Aliases 

# Shortcuts 
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"

# Functionality 
alias sudo='doas ' 

# Lunar Vim 
alias nvim="$HOME/.local/bin/lvim"

# Proton Executable for Windows apps
alias protonexec="$HOME/.local/share/Steam/steamapps/common/Proton\ -\ Experimental/files/bin/wine"
